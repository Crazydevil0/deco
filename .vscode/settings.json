{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "tailwindCSS.includeLanguages": {
    "tsx": "html",
    "html": "html",
    "javascript": "javascript",
    "typecript": "typescript",
    "css": "css"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  }
}
